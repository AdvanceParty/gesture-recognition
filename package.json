{
  "name": "tfjs-models",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/knn-classifier": "1.2.1",
    "@tensorflow-models/mobilenet": "^2.0.2",
    "@tensorflow/tfjs": "^1.2.1",
    "stats.js": "^0.17.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel src/index.html --no-hmr --open",
